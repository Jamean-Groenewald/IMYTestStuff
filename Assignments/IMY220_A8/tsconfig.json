{
    "compilerOptions": 
    {
      "target": "ES5",
      "lib": ["DOM", "ESNext"],
      "jsx": "react-jsx",
      "module": "ESNext",
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "strict": true,
      "skipLibCheck": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules", "dist"]
  }